{
  "name": "node-taglib-sharp-extend",
  "type": "module",
  "version": "0.2.7",
  "description": "read and write audio file metadata in nodejs / browser(memory)",
  "author": "subframe7536",
  "license": "LGPL-3.0-or-later",
  "repository": "https://github.com/subframe7536/node-taglib-sharp-memory",
  "keywords": [
    "music-metadata"
  ],
  "exports": {
    ".": {
      "types": "./dist/taglib/index.d.ts",
      "default": "./dist/taglib/index.js"
    },
    "./utils": {
      "import": "./dist/main/index.js",
      "require": "./dist/main/index.cjs"
    },
    "./vite": {
      "import": "./dist/main/vite.js",
      "require": "./dist/main/vite.cjs"
    }
  },
  "main": "./dist/taglib/index.js",
  "module": "./dist/taglib/index.js",
  "types": "./dist/taglib/index.d.ts",
  "typesVersions": {
    "*": {
      "utils": [
        "./dist/main/index.d.ts"
      ],
      "vite": [
        "./dist/main/vite.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "tsc --noEmit && eslint . --max-warnings 0",
    "format": "eslint . --fix",
    "dev": "tsup --watch",
    "build": "tsup && tsc -p ./tsconfig.taglib.json && node fix.mjs",
    "release": "pnpm test && pnpm run build && bumpp --all && npm publish",
    "test": "vitest --run",
    "test:dev": "vitest tests/bug.test.ts"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "peerDependencies": {
    "magic-string": ">=0.30",
    "vite-plugin-node-polyfills": ">=0.17.0"
  },
  "peerDependenciesMeta": {
    "vite-plugin-node-polyfills": {
      "optional": true
    }
  },
  "dependencies": {
    "iconv-lite": "^0.6.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@subframe7536/eslint-config": "^0.5.10",
    "@subframe7536/type-utils": "^0.1.6",
    "@types/node": "^20.11.25",
    "bumpp": "^9.4.0",
    "eslint": "^8.57.0",
    "eslint-plugin-solid": "^0.13.1",
    "node-taglib-sharp-memory": "github:subframe7536/node-taglib-sharp-mermory#04188810e92de0fdbf77243e058f8b9309654273",
    "tsup": "^8.0.2",
    "typescript": "^5.4.2",
    "vite": "^5.1.5",
    "vitest": "^1.3.1"
  },
  "pnpm": {
    "overrides": {
      "assert": "npm:@nolyfill/assert@latest",
      "is-typed-array": "npm:@nolyfill/is-typed-array@latest",
      "which-typed-array": "npm:@nolyfill/which-typed-array@latest"
    }
  }
}
