{
  "name": "node-taglib-sharp-extend",
  "type": "module",
  "version": "0.3.2",
  "description": "read and write audio file metadata in nodejs / browser(memory)",
  "author": "subframe7536",
  "license": "LGPL-3.0-or-later",
  "repository": "https://github.com/subframe7536/node-taglib-sharp-memory",
  "keywords": [
    "music-metadata"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./vite": "./dist/vite.js",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "vite": [
        "./dist/vite.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16.7.0"
  },
  "scripts": {
    "format": "eslint . --fix",
    "dev": "tsdown --stub --watch",
    "build": "tsdown",
    "release": "pnpm run build && bumpp --all && npm publish",
    "test": "pnpm run build && vitest --run"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "peerDependencies": {
    "magic-string": ">=0.30",
    "vite-plugin-node-polyfills": ">=0.17.0"
  },
  "peerDependenciesMeta": {
    "magic-string": {
      "optional": true
    },
    "vite-plugin-node-polyfills": {
      "optional": true
    }
  },
  "dependencies": {
    "@subframe7536/iconv-lite": "^0.7.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@subframe7536/eslint-config": "^1.3.2",
    "@subframe7536/type-utils": "^0.2.0",
    "@types/node": "^22.17.2",
    "bumpp": "^10.2.3",
    "eslint": "^9.33.0",
    "eslint-plugin-solid": "^0.14.5",
    "fast-base64": "^0.1.8",
    "tsdown": "^0.14.1",
    "typescript": "^5.9.2",
    "unplugin-inline-enum": "^0.4.1",
    "vite": "^7.1.2",
    "vite-plugin-node-polyfills": "^0.24.0",
    "vitest": "^3.2.4"
  },
  "pnpm": {
    "overrides": {
      "assert": "npm:@nolyfill/assert@^1",
      "is-core-module": "npm:@nolyfill/is-core-module@^1",
      "is-typed-array": "npm:@nolyfill/is-typed-array@^1",
      "which-typed-array": "npm:@nolyfill/which-typed-array@^1"
    },
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
