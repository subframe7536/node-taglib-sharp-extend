{
  "name": "node-taglib-sharp-extend",
  "type": "module",
  "version": "0.3.2-beta.1",
  "description": "read and write audio file metadata in nodejs / browser(memory)",
  "author": "subframe7536",
  "license": "LGPL-3.0-or-later",
  "repository": "https://github.com/subframe7536/node-taglib-sharp-memory",
  "keywords": [
    "music-metadata"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./vite": {
      "import": "./dist/vite.js",
      "require": "./dist/vite.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "vite": [
        "./dist/vite.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16.7.0"
  },
  "scripts": {
    "format": "eslint . --fix",
    "dev": "tsup --watch",
    "build": "tsup",
    "release": "pnpm run build && bumpp --all && npm publish",
    "test": "pnpm run build && vitest --run"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "peerDependencies": {
    "magic-string": ">=0.30",
    "vite-plugin-node-polyfills": ">=0.17.0"
  },
  "peerDependenciesMeta": {
    "magic-string": {
      "optional": true
    },
    "vite-plugin-node-polyfills": {
      "optional": true
    }
  },
  "dependencies": {
    "@subframe7536/iconv-lite": "^0.7.3",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@subframe7536/eslint-config": "^0.9.7",
    "@subframe7536/type-utils": "^0.1.6",
    "@types/node": "^22.9.3",
    "bumpp": "^9.8.1",
    "eslint": "^9.15.0",
    "eslint-plugin-solid": "^0.14.4",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "unplugin-inline-enum": "^0.2.0",
    "vite": "^5.4.11",
    "vitest": "^2.1.5"
  },
  "pnpm": {
    "overrides": {
      "is-arguments": "npm:@nolyfill/is-arguments@^1",
      "is-core-module": "npm:@nolyfill/is-core-module@^1",
      "is-generator-function": "npm:@nolyfill/is-generator-function@^1",
      "side-channel": "npm:@nolyfill/side-channel@^1"
    }
  }
}
